     "text": [
      "[[ 1.  0.  1.]\n",
      " [-3.  1. -1.]\n",
      " [ 0. -1. -1.]]\n",
      "[[ 1.  0.  0.]\n",
      " [-3.  1.  1.]\n",
      " [ 0.  0. -2.]]\n",
      "[-1. -3.  1.]\n",
      "[0. 0. 1.]\n"
     ]
    }
   ],
   "source": [
    "A1 = array([[1.,0.,1.],[-3.,1.,-1.],[0.,-1.,-1.]])\n",
    "\n",
    "invA1=linalg.inv(A1)\n",
    "\n",
    "A2 = array([[1.,0.,0.],[-3,1.,1.],[0. ,0.,-2.0]])\n",
    "\n",
    "x2 = array([0.0,0.0,1.0])\n",
    "\n",
    "AinvA = matmul(invA1,A2)\n",
    "\n",
    "x1 = -matmul(AinvA,x2)\n",
    "\n",
    "print(A1)\n",
    "print(A2)\n",
    "print(x1)\n",
    "print(x2)"
   ]
